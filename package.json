{
  "name": "neurobase",
  "version": "1.0.0",
  "description": "An intelligent, self-learning conversational database built on Agentic Postgres",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "start": "npm run build && node dist/cli.js",
    "serve": "npm run build && node dist/api.js",
    "init": "npm run build && node dist/scripts/init-db.js",
    "test": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:all": "jest --coverage --verbose",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "postgres",
    "ai",
    "nlp",
    "sql",
    "database",
    "tiger-data",
    "agentic",
    "llm",
    "natural-language"
  ],
  "author": "4n0nn43x",
  "repository": {
    "type": "git",
    "url": "https://github.com/4n0nn43x/neurobase.git"
  },
  "bugs": {
    "url": "https://github.com/4n0nn43x/neurobase/issues"
  },
  "homepage": "https://github.com/4n0nn43x/neurobase#readme",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.13.1",
    "dotenv": "^16.4.5",
    "commander": "^12.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6",
    "cli-table3": "^0.6.5",
    "openai": "^4.73.0",
    "@anthropic-ai/sdk": "^0.32.1",
    "ollama": "^0.5.13",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "express-rate-limit": "^7.4.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.23.8",
    "pgvector": "^0.2.0",
    "@xenova/transformers": "^2.17.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.14",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "eslint": "^8.57.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
